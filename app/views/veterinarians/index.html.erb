<h1>All veterinarians</h1>
<% @veterinarians.each do |vet| %>
  <section>
  <h3><%= link_to vet.name, "/veterinarians/#{vet.id}" %></h3>
  <p>Rating: <%= vet.review_rating %></p>
  <p>Practice: <%= vet.office_name %></p>
  <%= button_to "Edit #{vet.name}", "/veterinarians/#{vet.id}/edit", method: :get %>
  <%= button_to "Delete #{vet.name}", "/veterinarians/#{vet.id}", method: :delete %>
  </section>
<% end %>
